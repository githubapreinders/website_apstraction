 $(document).ready(function(){
        $("#modal_impatient").click(function(){
        if(window.screen.availWidth > 800)
        {
            $("#modal_impatient").modal("toggle");
        }
        else
        {
            window.location.href ="modal_impatient.html";
        }
        });
    });

